{
  "editor.fontFamily": "Fira Code, Consolas, monospace",
  "editor.fontLigatures": true,
  "editor.mouseWheelZoom": true,
  "editor.trimAutoWhitespace": false,
  "workbench.iconTheme": "vscode-icons",
  "trailing-spaces.includeEmptyLines": false,
  "trailing-spaces.trimOnSave": true,
  "explorer.confirmDragAndDrop": false,
  "[json]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[go]": {
    "editor.formatOnSave": false
  },
  "files.eol": "\n",
  "explorer.confirmDelete": false,
  "launch": {
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Nodemon (F)",
        "runtimeExecutable": "nodemon",
        "program": "${file}",
        "args": [  "-e", "js" ],
        "restart": true
      },
      {
        "type": "node",
        "request": "launch",
        "name": "Node.js (F)",
        "program": "${file}"
      },
      {
        "type": "python",
        "request": "launch",
        "name": "Python (F)",
        "program": "${file}"
      },
      {
        "type": "go",
        "request": "launch",
        "name": "Go (D)",
        "mode": "debug",
        "program": "${workspaceRoot}"
      }
    ]
  },
  "html.format.enable": false,
  "json.format.enable": false,
  "javascript.format.enable": false,
  "typescript.format.enable": false,
  "html.validate.scripts": false,
  "html.validate.styles": false,
  "php.validate.enable": false,
  "javascript.validate.enable": false,
  "typescript.validate.enable": false,
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "eslint.options": {
    "useEslintrc": false,
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2018,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "no-compare-neg-zero": "error",
      "no-cond-assign": "error",
      "no-constant-condition": "error",
      "no-dupe-args": "error",
      "no-dupe-keys": "error",
      "no-duplicate-case": "error",
      "no-empty-character-class": "error",
      "no-ex-assign": "error",
      "no-extra-boolean-cast": "warn",
      "no-extra-semi": "warn",
      "no-func-assign": "error",
      "no-invalid-regexp": "error",
      "no-irregular-whitespace": "error",
      "no-obj-calls": "error",
      "no-regex-spaces": "warn",
      "no-template-curly-in-string": "warn",
      "no-unreachable": "error",
      "no-unsafe-negation": "warn",
      "use-isnan": "error",
      "valid-typeof": "error",
      "array-callback-return": "error",
      "block-scoped-var": "error",
      "curly": [ "warn", "multi" ],
      "dot-location": [ "warn", "property" ],
      "no-else-return": "warn",
      "no-caller": "error",
      "no-empty-function": "error",
      "no-empty-pattern": "error",
      "no-extra-bind": "warn",
      "no-extra-label": "warn",
      "no-implicit-coercion": "warn",
      "no-multi-spaces": "warn",
      "no-multi-str": "error",
      "no-new-func": "error",
      "no-new-wrappers": "error",
      "no-octal-escape": "error",
      "no-param-reassign": "error",
      "no-redeclare": "error",
      "no-self-assign": "error",
      "no-self-compare": "error",
      "no-sequences": "error",
      "no-unused-expressions": "warn",
      "no-unused-labels": "warn",
      "no-useless-concat": "error",
      "no-useless-escape": "error",
      "no-useless-return": "warn",
      "radix": [ "error", "as-needed" ],
      "require-await": "error",
      "wrap-iife": "warn",
      "yoda": "warn",
      "no-delete-var": "error",
      "no-label-var": "error",
      "no-unused-vars": [ "error", { "args": "after-used" } ],
      "no-undef-init": "warn",
      "no-buffer-constructor": "error",
      "no-new-require": "error",
      "array-bracket-newline": [ "warn", "consistent" ],
      "array-bracket-spacing": [ "warn", "always" ],
      "block-spacing": "warn",
      "brace-style": [ "warn", "1tbs" ],
      "camelcase": "error",
      "comma-dangle": "warn",
      "comma-spacing": "warn",
      "computed-property-spacing": "warn",
      "func-call-spacing": "warn",
      "function-paren-newline": [ "warn", "never" ],
      "indent": [ "warn", "tab" ],
      "jsx-quotes": "warn",
      "key-spacing": "warn",
      "keyword-spacing": "warn",
      "linebreak-style": "warn",
      "lines-between-class-members": "warn",
      "max-statements-per-line": [ "error", { "max": 2 } ],
      "multiline-ternary": [ "error", "never" ],
      "new-parens": "warn",
      "no-array-constructor": "error",
      "no-lonely-if": "warn",
      "no-mixed-operators": "error",
      "no-mixed-spaces-and-tabs": "error",
      "no-multiple-empty-lines": [ "warn", { "max": 1, "maxBOF": 0, "maxEOF": 0 } ],
      "no-negated-condition": "error",
      "no-new-object": "error",
      "no-unneeded-ternary": "warn",
      "no-whitespace-before-property": "warn",
      "nonblock-statement-body-position": "warn",
      "object-curly-spacing": [ "warn", "always", { "arraysInObjects": true, "objectsInObjects": true } ],
      "one-var": [ "error", { "initialized": "never", "uninitialized": "always" } ],
      "operator-assignment": "warn",
      "quote-props": [ "warn", "as-needed", { "numbers": false } ],
      "quotes": [ "warn", "single" ],
      "semi": "warn",
      "semi-spacing": "warn",
      "semi-style": "warn",
      "space-before-blocks": "warn",
      "space-before-function-paren": [ "warn", { "anonymous": "never", "named": "never", "asyncArrow": "always" } ],
      "space-in-parens": "warn",
      "space-infix-ops": "warn",
      "space-unary-ops": "warn",
      "switch-colon-spacing": "warn",
      "template-tag-spacing": "warn",
      "unicode-bom": "warn",
      "wrap-regex": "warn",
      "arrow-body-style": "warn",
      "arrow-parens": "warn",
      "arrow-spacing": "warn",
      "constructor-super": "error",
      "generator-star-spacing": [ "warn", { "before": false, "after": true } ],
      "no-class-assign": "error",
      "no-const-assign": "error",
      "no-dupe-class-members": "error",
      "no-duplicate-imports": "error",
      "no-new-symbol": "error",
      "no-this-before-super": "error",
      "no-useless-computed-key": "warn",
      "no-useless-constructor": "error",
      "no-useless-rename": "warn",
      "no-var": "warn",
      "object-shorthand": "warn",
      "prefer-const": "warn",
      "prefer-rest-params": "error",
      "prefer-spread": "warn",
      "require-yield": "error",
      "rest-spread-spacing": "warn",
      "template-curly-spacing": "warn",
      "yield-star-spacing": [ "warn", { "before": false, "after": true } ],
      "react/no-did-mount-set-state": "error",
      "react/no-did-update-set-state": "error",
      "react/no-direct-mutation-state": "error",
      "react/no-redundant-should-component-update": "error",
      "react/no-will-update-set-state": "error",
      "react/prefer-es6-class": "error",
      "react/jsx-curly-spacing": [ "warn", { "children": true } ],
      "react/jsx-equals-spacing": "warn",
      "react/jsx-first-prop-new-line": "warn",
      "react/jsx-no-undef": "error",
      "react/jsx-no-bind": "error",
      "react/jsx-pascal-case": "error",
      "react/jsx-tag-spacing": [ "warn", { "beforeClosing": "never" } ],
      "react/jsx-uses-vars": "error",
      "react/jsx-uses-react": "error"
    }
  },
  "eslint.autoFixOnSave": true,
  "bookmarks.navigateThroughAllFiles": true
}
